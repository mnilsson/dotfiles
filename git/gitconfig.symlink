[user]
	name = Markus Nilsson
	email = markus@mnilsson.se
[alias]
  st = status -sb


  head = !git l -1
  h = !git head
  hp = "!. ~/.githelpers && show_git_head"
  l = "!. ~/.githelpers && pretty_git_log"


[color]
  status = auto
  branch = auto
  interactive = auto
  repositoryformatversion = 0
  filemode = true
  bare = false
  logallrefupdates = true
[push]
	default = simple
